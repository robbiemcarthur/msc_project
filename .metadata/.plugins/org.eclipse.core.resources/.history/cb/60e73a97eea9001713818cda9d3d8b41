package FormatIO;
import FormatIO.*;
import java.util.*;
import java.lang.*;


public class Ex1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Console con = new Console();
		Random rand = new Random();
		int num = Math.abs(rand.nextInt()) % 10;
		con.print("Guess a number: ");
		int guess = con.readInt();
		int count = 0;

		while (guess != num);
			if (guess == num)
			{
				con.println("Well done, you get the correct answer");
				count++;
			}
			else if (guess < num)
			{
				con.println("Too low");
				count++;
			}
			else 
			{
				con.println("Too high");
				count++;
			}
	}




}
