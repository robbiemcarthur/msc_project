package StudentGraph;

public class KnowledgeGraphFactory implements GraphFactory {
	KnowledgeGraph g;
	Student s;
	
	@Override
	public Graph createGraph() {
		g = new KnowledgeGraph();
		return g;
	}
	
	public Graph createStudentGraph(Student s) {
		this.s = s;
		g = new KnowledgeGraph();
		return g;
	}

}
