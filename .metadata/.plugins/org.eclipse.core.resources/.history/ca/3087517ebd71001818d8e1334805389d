
public abstract class Student {
	private String name, course, module, concept;
	private int id, conceptGrade;

	public Student(String n, String c, String m, String con, int i, int cg) {
		this.name = n;
		this.course = c;
		this.module = m;
		this.concept = con;
		this.id = i;
		this.conceptGrade = cg;
	}

	public Student() {
		name = "";
		course = "";
		module = "";
		concept = "";
		id = 0;
		conceptGrade = 0;
	}

	// SETTERS
	public void setName(String n) {
		this.name = n;
	}

	public void setCourse(String c) {
		this.course = c;
	}
	public void setModule(String m) {
		this.module = m;
	}
	public void setConcept(String con) {
		this.concept = con;
	}
	public void setName(String n) {
		this.name = n;
	}
	public void setName(String n) {
		this.name = n;
	}

}
