
public class pp2016 {

	public static class PrintThread extends Thread {
		private int mynumber;
		public PrintThread(int n) {
			mynumber = n;
		}
		public void run() {
			System.out.println("I am thread " + mynumber);
		}
	}
	public static void main(String[] args) {
		PrintThread[] threadSet1 = new PrintThread[100];
		PrintThread[] threadSet2 = new PrintThread[100];
		for(int i=0;i<100;i++) {
			threadSet1[i] = new PrintThread(1);
			threadSet2[i] = new PrintThread(2);
		}
		for(int i=0;i<100;i++) {
			threadSet1[i].start();
			threadSet2[i].start();
		}
		System.out.println("Finished!");
		Integer a = 2;
		 IntChange(a);
		 System.out.println(a); // Line A
	}
	public static void IntChange(Integer x) {
		x += 3;
	}
	{
	}

}



